{
    "name": "natanphp/framework",
    "description": "Framework PHP MVC simple, moderno e innovador para aprendizaje",
    "version": "0.2.1",
    "type": "framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Jhonatan FernÃ¡ndez",
            "email": "jhonatanfdez@example.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "NatanPHP\\Core\\": "core/",
            "NatanPHP\\App\\": "app/"
        },
        "files": [
            "core/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "NatanPHP\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "test-unit": "phpunit --testsuite unit",
        "test-feature": "phpunit --testsuite feature"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}